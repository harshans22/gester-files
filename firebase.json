{
  "hosting": [
    {
      "target": "gester",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/privacypolicy",
          "destination": "/index.html"
        }
      ],
      "redirects": [
        {
          "source": "/index.html",
          "destination": "/privacypolicy",
          "type": 301
        }
      ]
    }
  ]
}
